
config wifidog 'wifidog'
	option gateway_interface 'br-lan'
	option gateway_eninterface 'eth0'
	option gateway_httpport '80'
	option gateway_connmax '130'
	option HTTPDMaxConn '15'
	option check_interval '30'
	option client_timeout '5'
	option sxsd '1'
	option xxsd '2'
	option wifidog_enable '1'
	option deamo_enable '1'
	option white_black_enable '1'
	option gateway_path '/'
	list bmd_url 'pushdog.org'
	option gatewayport '3000'

config wifidog_deamo 'wifidog_deamo'
	option enable '1'

